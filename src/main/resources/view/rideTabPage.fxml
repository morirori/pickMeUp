<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="369.0" prefWidth="1058.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.michal.pickMeUp.Controllers.RideTabController">
   <children>
      <Button fx:id="createRideButton" layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onAction="#createRideActionHandler" prefHeight="25.0" prefWidth="114.0" text="Create ride" />
      <TableView fx:id="rideTabView" layoutX="146.0" layoutY="12.0" prefHeight="346.0" prefWidth="907.0">
         <columns>
            <TableColumn fx:id="rideIDsTable" editable="false" prefWidth="50.0" resizable="false" text="RideID" />
            <TableColumn fx:id="ownerTable" editable="false" prefWidth="53.0" resizable="false" text="Owner" />
            <TableColumn fx:id="startTable" editable="false" prefWidth="103.0" resizable="false" text="Start" />
            <TableColumn fx:id="endTable" editable="false" prefWidth="89.0" resizable="false" text="End" />
            <TableColumn fx:id="finishedTable" editable="false" prefWidth="69.0" resizable="false" text="Finished" />
            <TableColumn fx:id="startTimeTable" editable="false" prefWidth="178.0" resizable="false" text="Start time" />
            <TableColumn fx:id="freeSpacesTable" editable="false" prefWidth="72.0" resizable="false" text="Free spaces" />
            <TableColumn fx:id="costTable" editable="false" minWidth="0.0" prefWidth="144.0" resizable="false" text="Cost" />
            <TableColumn fx:id="distanceTxtBox" prefWidth="75.0" text="Distance" />
            <TableColumn fx:id="timeTxtBox" prefWidth="75.0" text="Time" />
         </columns>
      </TableView>
      <Button fx:id="joinRideButton" layoutX="14.0" layoutY="60.0" mnemonicParsing="false" onAction="#joinRideActionHandler" prefHeight="25.0" prefWidth="114.0" text="Join ride" />
      <TextField fx:id="rideIDButton" layoutX="14.0" layoutY="100.0" prefHeight="25.0" prefWidth="114.0" />
      <Button fx:id="refreshButton" layoutX="14.0" layoutY="330.0" mnemonicParsing="false" onAction="#refreshActionHandler" prefHeight="25.0" prefWidth="114.0" text="Refresh" />
      <TextField fx:id="userIDTxt" layoutX="14.0" layoutY="180.0" prefHeight="25.0" prefWidth="114.0" />
      <Button fx:id="inspectUserButton" layoutX="14.0" layoutY="140.0" mnemonicParsing="false" onAction="#inspectUserHandler" prefHeight="25.0" prefWidth="114.0" text="Inspect user" />
   </children></AnchorPane>
